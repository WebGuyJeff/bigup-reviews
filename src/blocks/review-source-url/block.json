{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "bigup-reviews/review-source-url",
	"version": "0.1.0",
	"title": "Source Link",
	"category": "theme",
	"icon": "smiley",
	"description": "Source URL custom field for the 'Review' custom post type",
	"usesContext": [ "postId", "postType", "queryId" ],
	"textdomain": "bigup-reviews",
	"editorScript": "file:./index.js",
	"supports": { "html": false },
	"ancestor": [ "bigup-reviews/review" ],
	"attributes": {
		"linkText": {
			"type": "string",
			"default": "See original review",
			"source": "text",
			"selector": "a"
		}
	}
}